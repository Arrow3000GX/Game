[sub_resource type="GDScript" id="GDScript_a7uq4"]
script/source = "extends CharacterBody2D

var SPEED = 500
var direction: int = 1
var has_hit = false
var stopped = false

@onready var anim = $AnimationPlayer
@onready var sprite = $AnimatedSprite2D

func _ready() -> void:
	anim.play(\"Attack\")
	sprite.flip_h = direction <= 0

func _on_animation_player_animation_finished(anim_name: StringName) -> void:
	if anim_name == \"Attack\":
		queue_free()	





'''
func _ready() -> void:
	anim.play(\"Flying ball\")
	sprite.flip_h = direction < 0

func _physics_process(_delta: float) -> void:
	if not stopped:
		velocity = Vector2(direction * SPEED, 0)
	move_and_slide()


func _on_fireball_area_entered(area: Area2D) -> void:
	if has_hit:
		return
	if area.name != \"DetectionArea\" and area.name != \"AttackArea\" and area.name != \"cherry\" and area.name != \"Fireball\" and area.name != \"Player\":
		has_hit = true
		stopped = true
		velocity = Vector2(0,0)
		anim.play(\"hit\")
		velocity = Vector2(0,0)
		await get_tree().create_timer(0.05).timeout
		$CollisionShape2D.call_deferred(\"set_disabled\", true)
func _on_animation_player_animation_finished(anim_name: StringName) -> void:
	if anim_name == \"hit\":
		queue_free()
\"\"\"
"

